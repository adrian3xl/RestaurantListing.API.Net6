{
  "ConnectionStrings": {

    "ResturantListingDBConnectionString": "Server=(localdb)\\mssqllocaldb;Database=ResturantListingAPIDB; Trusted_Connection=True,MultipleActiveResultSet=True"
  },



  "Serilog": {

    "MinimumLevel": {

      "Default": "Information",
      "Override": {

        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"

      }
    },

    "WriteTo": [
      {
        "Name": "File",


        "Args": {

          "path": "./logs/log-.tex",
          "rollingIntervals": "Day"
        }
      },

      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]


  },

  "AllowedHosts": "*"
}
